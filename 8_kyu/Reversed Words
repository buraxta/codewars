public class ReverseWords{

public static String reverseWords(String str){
     String[] newList = str.split(" ");
     String reversed = "";
     for(int i = newList.length-1; i>=0 ;i-- ){
         reversed += newList[i];
         if(i == 0){
             continue;
         }
         reversed += " ";
         
     }
     return reversed;
 }
}
