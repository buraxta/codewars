public class Bio {
    public static String dnaToRna(String dna) {
        char[] given = dna.toCharArray();
        for(int i = 0; i<dna.length(); i++){
            if(given[i] == 'T'){
                given[i] = 'U';
            }        
        }        
        return String.valueOf(given);
    }
}
